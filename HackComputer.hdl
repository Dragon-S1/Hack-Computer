CHIP HackComputer {

    IN reset;

    PARTS:

    ROM32K(address=pc, out=instruction);
    HackCPU(inM=memOut, instruction=instruction, reset=reset, outM=outM, writeM=writeM, addressM=addressM, pc=pc);
    Memory(in=outM, load=writeM, address=addressM, out=memOut);
}